<h1>Listing rush_applications</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Sex</th>
    <th>Address</th>
    <th>Phone number</th>
    <th>Mobile number</th>
    <th>Email</th>
    <th>Question1</th>
    <th>Question2</th>
    <th>Question3</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rush_applications.each do |rush_application| %>
  <tr>
    <td><%= rush_application.name %></td>
    <td><%= rush_application.sex %></td>
    <td><%= rush_application.address %></td>
    <td><%= rush_application.phone_number %></td>
    <td><%= rush_application.mobile_number %></td>
    <td><%= rush_application.email %></td>
    <td><%= rush_application.question1 %></td>
    <td><%= rush_application.question2 %></td>
    <td><%= rush_application.question3 %></td>
    <td><%= link_to 'Show', rush_application %></td>
    <td><%= link_to 'Edit', edit_rush_application_path(rush_application) %></td>
    <td><%= link_to 'Destroy', rush_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Rush application', new_rush_application_path %>
